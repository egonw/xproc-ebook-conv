<!-- DBNL special characters for HTML -->

<!-- Character entity set. Typical invocation:
     <!ENTITY % DBNLsymbol PUBLIC
       "-//DBNL//ENTITIES Special//EN//HTML">
     %DBNLspecial; -->

<!ENTITY unicode  "[unicode]"><!-- Unicode note -->
<!ENTITY schwa    "[schwa ]"><!-- Schwa -->
<!ENTITY aolduml  "[aolduml]"><!-- a Old Umlaut -->
<!ENTITY eolduml  "[eolduml]"><!-- e Old Umlaut -->
<!ENTITY oolduml  "[oolduml]"><!-- o Old Umlaut -->
<!ENTITY uolduml  "[uolduml]"><!-- u Old Umlaut -->
